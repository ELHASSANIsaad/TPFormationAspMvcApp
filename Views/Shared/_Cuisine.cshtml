@{
}

@model List<FormationAspMvcApp.Models.Cuisins>

<dl class="row">

    @foreach(var cuisine in Model)
    {
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => cuisine.Id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayNameFor(model => cuisine.Name)

        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => cuisine)

        </dt>
    }

</dl>
